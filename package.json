{
  "name": "@dataverse/hooks",
  "version": "1.0.31",
  "description": "React hooks of dataverse app.",
  "types": "dist/esm/index.d.ts",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "/dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:esm": "tsc -p tsconfig.json",
    "build:cjs": "tsc -p tsconfig-cjs.json",
    "build": "rm -rf ./dist && pnpm build:esm && pnpm build:cjs",
    "buildForWindows": "del /s /q dist && pnpm build:esm && pnpm build:cjs",
    "format": "prettier --config .prettierrc . --write",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@dataverse/dataverse-connector": "^1.0.30",
    "@dataverse/dataverse-contracts-sdk": "^1.0.17-alpha.13",
    "@dataverse/model-parser": "^1.0.2",
    "@dataverse/wallet-adapter": "^1.0.3",
    "react": "17.0.2",
    "typescript": "^5.1.6",
    "viem": "^1.12.1",
    "wagmi": "^1.3.9"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.196",
    "@types/react": "17.0.2",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.1",
    "husky": "^8.0.3",
    "prettier": "^3.0.0"
  }
}
